<odoo>
  <data>
    <record id="inherit_pos_config_view_form" model="ir.ui.view">
      <field name="name">pos.config.view.form.inherit</field>
      <field name="model">pos.config</field>
      <field name="inherit_id" ref="point_of_sale.pos_config_view_form"/>
      <field name="arch" type="xml">
        <xpath expr="//div[@class='row mt16 o_settings_container'][2]/div[@class='col-12 col-lg-6 o_setting_box'][2]" position="after">
          <div class="col-12 col-lg-6 o_setting_box">
            <div class="o_setting_left_pane">
              <field name="is_cash_box"/>
            </div>
            <div class="o_setting_right_pane">
              <label for="is_cash_box"/>
            </div>
          </div>
        </xpath>
      </field>
    </record>
  </data>
</odoo>